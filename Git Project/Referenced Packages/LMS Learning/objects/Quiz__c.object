    <validationRules>
        <fullName>Not_Valid_Duration_Value</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(DurationTime__c , &quot;(((((\\d+?[d]\\b)\\s?){0,1})|\\d{0,4})?((((\\d{1,2}?[h]\\b)\\s?){0,1})|\\d{0,4})?((((\\d{1,2}?[m]\\b)\\s?){0,1})|\\d{0,4})?)&quot;))</errorConditionFormula>
        <errorDisplayField>DurationTime__c</errorDisplayField>
        <errorMessage>NOT VALID DURATION</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Passing_Score</fullName>
        <active>true</active>
        <description>Passing Score must be between 0 and 100%.</description>
        <errorConditionFormula>OR(
Passing_Score__c &lt; 0,
Passing_Score__c &gt; 100
)</errorConditionFormula>
        <errorDisplayField>Passing_Score__c</errorDisplayField>
        <errorMessage>Passing Score must be between 0 and 100%.</errorMessage>
    </validationRules>
