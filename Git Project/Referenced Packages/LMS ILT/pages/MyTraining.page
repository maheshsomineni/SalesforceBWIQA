		reloadpage = 1;
	}

	var description;
	var preSequenceComplete;

	function fillIntermidiateVariable(seq, descr,type, objId) {
		preSequenceComplete =seq;
		description = descr;
		//alert('seq = ' + seq + ' descr= ' + descr.split("\\n").join("\n") + 'type= ' +type + ' objId =' + objId);
		showEdit(type, objId);
	}

	function checkSequence() {
		var information = new Object();
		information.descr = (description === undefined || description === '') ? undefined:description;
		information.seq = (preSequenceComplete === undefined || preSequenceComplete === '') ? undefined :  preSequenceComplete;
		return information;
	}

