	window.alert = function(msg) {

		msg = msg.replace(/\'/g,"&apos;");
		msg = msg.replace(/\"/g,"&quot;");
		msg = msg.replace(/</g,"&lt;");
		msg = msg.replace(/>/g,"&gt;");
		msg = msg.replace(/\n/g,"<br>");

		jQuery('#popup_content').html(msg);
		Lightbox();
	}
