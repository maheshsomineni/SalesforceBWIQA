									jQuery('td.m-col5[name="'+arr2[0]+'"]').text(arr2[3]);
								}
							}
						}
					} if({!lmsConfig.Show_progress_bar} && {!lmsConfig.Show_Score}) {
						if (statusObject[arr2[0]].progress != arr2[2] || statusObject[arr2[0]].score != arr2[3]) {
