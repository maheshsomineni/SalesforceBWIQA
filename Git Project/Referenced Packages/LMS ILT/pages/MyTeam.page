<apex:page controller="lmsilt.MyTeam" title="{!$Label.lmscons__MyTeam_Header}" sidebar="false" id="page" showheader="{!RegularMyTeam}">
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8"/>
    <apex:includeScript value="{!URLFOR($Resource.lmscons__MyTeam, 'js/jquery-1.6.4.js')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.lmscons__MyTeam, 'css/team.css')}"/>
    <apex:stylesheet value="{!URLFOR($Resource.lmscons__jquery1816min, 'jquery-ui.css')}" />
    <apex:includeScript value="{!URLFOR($Resource.lmscons__jquery1816min, 'jquery-ui.min.js')}" />    
    <apex:includeScript value="{!URLFOR($Resource.lmsilt__devplan, 'js/init.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.lmsilt__devplan, 'js/jsapi.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.lmsilt__lmslightbox, 'js/init.js')}"/>            
    <apex:stylesheet value="{!URLFOR($Resource.lmsilt__devplan, 'css/lightbox.css')}"/>    

    <style type="text/css">
    #tsid 					{ z-index:200}
	.clearAll				{clear: both;}

    .small-like-icon {
        background: url("/img/chatter/likesmall.png") repeat scroll 0 0 transparent;
        display: inline-block;
        height: 12px;
        margin-right: 6px;
        width: 12px;
    }

    .sml {
        font-size: 0.92em;
        font-weight: bold;
    }

    .ui-widget-overlay {
        background: #010101;
        opacity: 0.6;
        filter:alpha(opacity=60);
        height:100%;
        width:100%;
        z-index: 1001;
    }
    .b-manage-task table {width:100%; margin-bottom:5px;}
        .b-manage-task table td {color:#575757; padding:5px 6px; border-bottom: 1px solid #C3C3C3; text-align:left}
        .b-manage-task table td.m-right {text-align:right}
        .b-manage-task a {color:#4D889A; text-decoration:none;}
        .b-manage-task a:hover {color:#4D889A; text-decoration:underline;}
        .b-manage-task .m-all {padding-left:5px;}
    </style>

    <script>
        function checkSequence() {
        var var2;
            return undefined;
        }
        
        function resizeIframe() {
		    var width = jQuery(window).width();
		    jQuery('.b-lightbox').css({
		     'width': width * 0.6+'px',
		     'left':  width * 0.2+'px',
		     'right': width * 0.2+'px',
		     'marginLeft':0
		    });
		} 
		   
		 function Lightbox() {				
			var lightbox = jQuery('div.b-lightbox');
			var overlay =  jQuery('div.b-overlay');
				lightbox.fadeIn('fast');
				overlay
					.height( jQuery(document).height() )
					.css('opacity','0.6')
					.fadeIn('fast');					
			return false;		
		}
		
		function EventDetails(type, objId, catalogId) {
	  		jQuery('#popup_content').html('<iframe onload="resizeIframe();" id="mytrainingframe" name="mytrainingframe" resizable="0" scrollbar="0" frameborder="0" height="350" src="{!$Page.MyTrainingPopup}?type=' + type + '&objId=' + objId + '&catalogId=' + catalogId + '&action=edit&checkSequence=false" width="100%"></iframe>');
	        resizeIframe();
	        Lightbox();
	    }
    </script>   
        	
        <apex:form id="formdraw">
            <apex:outputpanel id="task_popup" layout="none" rendered="{!RegularMyTeam = false}">
                <apex:actionFunction action="{!Refresh_Task_Popup}" name="Refresh_Task_Popup" rerender="task_popup" status="search_status" />

                <script type="text/javascript">
                    var isChanged = 0;
                    var idPage = '';
                    function someChanges(pageName) {
                        isChanged = 1;
                        idPage = pageName;
                    }

                    function open_task_lightbox(type, objId) {
                        jQuery('#task_details').html('<iframe frameborder="0" height="300px" name="mytr_popup_frame" src="{!$Page.MyTrainingPopup}?type=' + type + '&objId=' + objId + '&action=edit" width="100%"></iframe>');
                        jQuery('.b-lightbox,.b-overlay').show();
                    }

                    function close_lightbox() {
                        jQuery('.b-lightbox,.b-overlay').hide();
                        if(isChanged && idPage == "myTeam") {
                            Refresh_Task_Popup();
                        }
                    }

                    function checkSequence() {
                        return undefined;
                    }
                </script>

                <apex:pageMessages id="messages2"/>
                <apex:pageBlock title="{!$Label.MyTeam_Task_Manage}" id="task_section">

                    <apex:pageBlockSection id="filter" columns="5" title="{!$Label.Search}">
                        <apex:pageBlockSectionItem datastyle="width: 50px;" labelstyle="width: 100px;">
                            <apex:outputLabel value="{!$Label.MyTeam_Task_Trainee}" style="text-align:left;" />
                            <apex:selectList value="{!UserTypeValue}" multiselect="false" size="1">
                                <apex:selectOptions value="{!UserType}"/>
                            </apex:selectList>
                        </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem datastyle="width: 10px;" labelstyle="width: 10px;">
                            <apex:outputLabel />
                            <apex:inputText required="false" value="{!Trainee}" onkeypress="return runScript(event)"/>
                        </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem datastyle="width: 10px;" labelstyle="width: 10px;">
                            <apex:outputLabel />
                            <apex:commandButton action="{!Search_Task}" value="Search" id="searchbutton"/>
                        </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem datastyle="width: 10px;" labelstyle="width: 10px;">
                            <apex:outputLabel />
                            <apex:outputpanel layout="inline" style="position:relative; left:-20px; width: 20px;"> &nbsp;&nbsp;&nbsp;
                                <apex:actionStatus id="search_status">
                                    <apex:facet name="start">
                                        <apex:image url="/img/loading.gif" />
                                    </apex:facet>
                                </apex:actionStatus>
                            </apex:outputpanel>
                        </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem datastyle="width: 200px;" labelstyle="width: 200px;">
                        &nbsp;
                        </apex:pageBlockSectionItem>
                    </apex:pageBlockSection>

                    <apex:pageBlockTable value="{!TaskList}" id="tasklist" var="a" cellPadding="4" border="1" rendered="{!TaskList.size > 0}">
                        <apex:column >
                            <apex:facet name="header">Date</apex:facet>
                            <apex:outputText value="{0,date, MM/dd/yyyy}"><apex:param value="{!a.TaskDate}" /></apex:outputText>
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                {!$Label.MyTeam_Task_FullName}
                            </apex:facet>
                            {!a.Username}
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                {!$Label.MyTeam_Task_Title}
                            </apex:facet>
                            {!a.TaskName}
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                {!$Label.MyTeam_Task_Status}
                            </apex:facet>
                            {!a.Status}
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                    &nbsp;
                            </apex:facet>
                                <a href="javascript:void(0)" onclick="open_task_lightbox('{!$ObjectType.Development_Plan_Task__c.Name}', '{!a.AssignmentId}')" class="m-bold">{!$Label.Details}</a>
                        </apex:column>
                    </apex:pageBlockTable>
                </apex:pageBlock>
            </apex:outputpanel>

            <apex:outputpanel layout="none" rendered="{!RegularMyTeam = true}">

                <apex:outputPanel id="err">
                <h1 rendered="{!Error != ''}">{!Error}</h1>
                </apex:outputPanel>

                <apex:outputPanel rendered="{!Error = ''}" id="jsc">
                <script type="text/javascript">

                google.load("visualization", "1", {packages:["corechart"]});
                google.setOnLoadCallback(drawChart2);
                var current_size = {!ChatterPageSize}+1;
                var userid = '';

                function init() {

                    //Irina code start ---------------------------------------------------------------
                    Toggler();
                    SetHeight();
                    jQuery('.b-tabs .b-tab:first-child').addClass('m-first');
                    jQuery('.b-team-list .b-item:last-child').addClass('m-last');


                    jQuery('.b-tabs .b-tab').hover(
                        function () {jQuery(this).addClass('m-over');},
                        function () {jQuery(this).removeClass('m-over');}
                    );

                    jQuery('.b-tabs .b-tab.m-active:not(.m-first)').addClass('m-doc-icon');
                    jQuery('.b-tabs .b-tab.m-first:not(.m-active)').addClass('m-parent-icon');

                    // Irina code end ---------------------------------------------------------------

                    jQuery('#dialog-modal').dialog({
                        autoOpen: false,
                        width: 540,
                        height: 230,
                        modal: true,
                        resizable: false
                    });


                    jQuery('[id*="comment_click_"]').unbind('click');
                    jQuery('[id*="open_thread_"]').unbind('click');
                    jQuery('[id*="mlike_"]').unbind('click');

                    jQuery('[id*="comment_click_"]').click(function() {
                        var m = this.id;
                        m = m.replace(/comment_click_/ig, "");
                        //alert(jQuery('#comment_'+m).css('display'));
                        jQuery('#comment_'+m).toggle('slow');
                    });

                    jQuery('[id*="open_thread_"]').click(function() {
                        var m = this.id;
                        m = m.replace(/open_thread_/ig, "");
                        jQuery('#thread_comment_'+m).toggle('slow');
                    });

                    jQuery('[id*="mlike_"]').click(function() {
                        var m = this.id;
                        m = m.replace(/mlike_/ig, "");
                        likef(m);
                    });

                    jQuery('[id*="unlike_"]').click(function() {
                        var m = this.id;
                        m = m.replace(/unlike_/ig, "");
                        unlikef(m);
                    });

                    jQuery('[id*="trainingstatus_comment_click_"]').click(function() {

                        var m = this.id;
                        var person = jQuery("#"+m).text()

                        m = m.replace(/trainingstatus_comment_click_/ig, "");
                        userid = m;

                        jQuery("#frombox").html(person);

                        jQuery("#scm").val('');
                        jQuery("#dialog-modal").dialog('open');
                    });
                }

                function SendMesageToAll() {
                    jQuery("#scm").val('');
                    jQuery("#frombox").html('ALL');
                    userid = 'all';
                    jQuery("#dialog-modal").dialog('open');
                }

				//==============================================================================================
				//Popup with assignments for selected user
				//==============================================================================================
				function open_as_popup(userID, userName) {
					jQuery('#dialog-modal2').dialog({
						autoOpen: false,
						width: jQuery(window).width()*0.9,
						height: jQuery(window).height()*0.9,
						modal: true,
						title: 'just simple placeholder, correct value will be assign below',
						resizable: false
					});
					//to deal with jQuery Dialog caching issue we have to reassign some of dialog`s options values
					jQuery('#dialog-modal2').html('<iframe frameborder="0" height="100%" id="mytr_popup_frame" name="mytr_popup_frame" src="{!$Page.MyTraining}?overrideuser=' + userID + '&renderButtons=false&showCompletedDate=true" width="100%"></iframe>');
					jQuery("#dialog-modal2").dialog('option', 'title', getPopupTitle(userName));
					jQuery("#dialog-modal2").dialog('open');
				}
	
				function getPopupTitle(userName){
					return '{!$Label.lmscons__MyTeam_User_Assignments}'.replace("{0}", userName);
				}
				//===============================================================================================

                function open_tasks(user) {

                    jQuery('#dialog-modal4').html('<iframe frameborder="0" height="100%" id="task_popup_frame" name="task_popup_frame" src="{!$Page.MyTeam}?show_task_user='+user+'" width="100%"></iframe>');
                    jQuery('#dialog-modal4').dialog({
                        autoOpen: false,
                        width: jQuery(window).width()*0.9,
                        height: jQuery(window).height()*0.9,
                        modal: true,
                        title: 'Tasks',
                        resizable: false,
                        close: function(event, ui) {
                            if(isChanged && idPage == "myTeam") {
                                window.location.reload();
                            }
                        }
                    });

                    jQuery("#dialog-modal4").dialog('open');
                }


                function closesearch() {
                    jQuery("#dialog-modal3").dialog('close');

                }

                function open_assign_popup() {

                    jQuery('#dialog-modal2').dialog({
                        autoOpen: false,
                        width: jQuery(window).width()*0.9,
                        height: jQuery(window).height()*0.9,
                        modal: true,
                        resizable: false
                    });


                    jQuery("#dialog-modal2").dialog('open');
                }


                function open_sc_popup() {

                    var szx = jQuery(window).width()*0.9;
                    var szy = jQuery(window).height()*0.9-100;
                    var url = '{!$Page.SearchCatalogNew}';

                    if ({!ILTPackageInstalled} == true) {
                        url = '{!$Page.SearchCatalogNew}';
                    }

					jQuery('#dialog-modal3').dialog({
                        autoOpen: false,
                        width: jQuery(window).width()*0.9,
                        height: jQuery(window).height()*0.9,
                        modal: true,
                        resizable: false,
                        close: function(event, ui) {
                        	jQuery('#dialog-modal3').empty();
                            RefreshPage();
                        }
                    });
					
					
                    jQuery('#dialog-modal3').html('<iframe frameborder="0" height="'+szy+'px" id="theIframe" name="theIframe" src="'+url+'" width="100%"></iframe>');
                    open_search_catalog();
                }


                function open_search_catalog() {

                    

                    jQuery("#dialog-modal3").dialog('open');
                }



                    // Irina code start ---------------------------------------------------------------
                    function SetHeight() {
                        var tblscroll = jQuery('.b-scroll table');
                        if ( tblscroll.innerHeight() > 230) {
                                jQuery('.b-training-status .b-tbl table thead th:last-child').css({width:'32px'});
                        }
                        else {
                                jQuery('.b-training-status .b-tbl table thead th:last-child').css({width:'14px'});
                        }

                    }


                    /* Toggler */
                    function Toggler()
                    {
                        jQuery('.b-tab-bodies .b-tab-body').hide();
                        jQuery('.b-tab-bodies .b-tab-body:first-child').show();
                        jQuery('.b-tabs .b-tab').append('<span class="m-white-fix"></span>');
                    }

                    // Irina code end ---------------------------------------------------------------

                    function close_comment() {
                        jQuery("#dialog-modal").dialog('close');
                    }

                    function ShowMoreComments() {
                        var size = {!ChatterRec.size};
                        var increase = {!ChatterShowRecordsPerClick};

                        //alert('{!ChatterRec.size} '+(current_size+increase));

                        if ({!ChatterRec.size} <= (current_size+increase)) {
                            //alert('Ye!');
                            increase = {!ChatterRec.size} - current_size;
                            jQuery('#showmorelink').hide('slow');
                            }

                        for (i = current_size; i <= current_size+increase; i++) {
                            jQuery('#charter_'+i).show('slow');
                        }
                        current_size = current_size + increase;
                    }

                    function drawChart2() {
                        init();

                        var data = new google.visualization.DataTable();
                        data.addColumn('string', 'Task');
                        data.addColumn('number', 'Hours per Day');
                        data.addRows([
                            <apex:repeat value="{!ChData}" var="chart">
                                ["{!chart.Status}", {!chart.Value}],
                            </apex:repeat>
                                ['', 0]

                        ]);

                        var options = {
                            width: 250, height: 250,
                            title: '{!JSENCODE($Label.Record_Count)}',
                            legend: {position: 'bottom'},
                            colors:[<apex:repeat value="{!Colors}" var="c" >'{!c}', </apex:repeat>]
                        };

                        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
                        chart.draw(data, options);
                    }

                    function drawchart() {
                        drawChart2();
                    }


                    function send_comment() {

                        var cmbody = jQuery("#scm").val();

                        if (cmbody.length > 0) {
                            CommitComment3(userid,cmbody);
                            close_comment();
                        }
                        else {
                            alert('Comment Body is required');
                        }
                    }

                    var isChanged = 0;
                    var idPage = '';
                    function someChanges(pageName) {
                        isChanged = 1;
                        idPage = pageName;
                    }

                    function open_task_lightbox(type, objId) {
                        jQuery('#task_details').html('<iframe frameborder="0" height="300px" name="mytr_popup_frame" src="{!$Page.MyTrainingPopup}?type=' + type + '&objId=' + objId + '&action=edit" width="100%"></iframe>');
                        jQuery('.b-lightbox,.b-overlay').show();
                    }

                    function checkSequence() {
                        return undefined;
                    }

                    function close_lightbox(){
                        jQuery('.b-lightbox,.b-overlay').hide();
                        jQuery('#task_details, #popup_content').empty();
                        
                        if(isChanged && idPage == "myTeam") {
                            window.location.reload();
                        }
                    }
                    
                    function handleKeyPress(e){
         				var eventInstance = window.event ? event : e;
           				var keyCode = eventInstance.charCode ? eventInstance.charCode : eventInstance.keyCode;
            			if (keyCode == 13){
                			SearchUsersEnter();
                			return false;
            			}
        			}

                </script>

                <apex:actionFunction action="{!RefreshAssignPopup}" name="as_pop_refr" rerender="formdraw2" status="loadStatus" oncomplete="open_assign_popup()">
                    <apex:param name="as_pop_refr2" assignTo="{!AsgnUsrId}" value="" />
                </apex:actionFunction>

                <apex:actionFunction action="{!RefreshPage}" name="RefreshPage" rerender="formdraw" oncomplete="drawchart()" status="loadStatus" />

                <apex:actionFunction action="{!LikeFeed}" name="likef" rerender="formdraw" status="loadStatus" oncomplete="drawchart()">
                    <apex:param name="likevalue" assignTo="{!LikeValue}" value="" />
                </apex:actionFunction>

                <apex:actionFunction action="{!CommitComment3}" name="CommitComment3" rerender="formdraw" status="loadStatus" oncomplete="drawchart()">
                    <apex:param name="var1" assignTo="{!CommentTo}" value="" />
                    <apex:param name="var2" assignTo="{!Comment}" value="" />
                </apex:actionFunction>

                <apex:actionFunction action="{!UnLikeFeed}" name="unlikef" rerender="formdraw" status="loadStatus" oncomplete="drawchart()">
                    <apex:param name="likevalue" assignTo="{!LikeValue}" value="" />
                </apex:actionFunction>
                
                <apex:actionFunction action="{!SearchUsers}" name="SearchUsersEnter" reRender="formdraw" oncomplete="drawChart2();" status="loadStatus"/>

                <apex:actionStatus id="loadStatus">
                    <apex:facet name="start">
                        <c:EnhancedActionStatus BackColor="#efefef" borderColor="#336699" borderSize="3" height="50px" width="120px" ImageUrl="/img/loading.gif" Message="{!$Label.lmscons__Consumer_Global_Loading}..." messageStyle="color:darkred;font-size:11pt;font-weight:bold;"/>
                    </apex:facet>
                </apex:actionStatus>

            <div class="h-base">
                <div class="l-header">

                    <h1 class="b-title"><img src="{!URLFOR($Resource.lmscons__MyTeam, 'i/header-ico.png')}" />{!$Label.lmscons__MyTeam_Header}</h1>

                    <apex:outputpanel layout="block" styleClass="b-btns" rendered="{!isConsumerUser}">
                        <div class="m-training"><a class="b-btn" href="JavaScript:open_sc_popup();"><span class="b-icon">&nbsp;</span>{!$Label.lmscons__MyTeam_Assign_Training}</a></div>
                    </apex:outputpanel>

                    <span class="m-cfix"></span>

                    <apex:outputpanel layout="inline" rendered="{!isConsumerUser}">
                        <apex:inputCheckbox value="{!ShowInactive}" id="check_inactive" onclick="javascript:check_inactive();" />
                        <label for="page:formdraw:check_inactive">{!$Label.lmscons__MyTeam_Show_Inactive_Users}</label>
                    </apex:outputpanel>

                    <apex:outputpanel layout="inline">
                        <apex:pageMessages id="messages" />
                    </apex:outputpanel>

                    <apex:actionFunction action="{!ShowInactiveAction}" name="check_inactive" />

                </div>

                <apex:outputpanel layout="block" styleClass="l-content" rendered="{!isConsumerUser}">
                    <div class="b-content">
                        <div class="b-tabs">

                            <apex:outputpanel layout="block" rendered="{!RealUserName != SelectedUserName}" styleclass="b-tab m-parent-icon m-has-child m-first">
                                <apex:commandLink value="{!RealUserName}" action="{!SelectUser}" status="loadStatus" oncomplete="drawchart()" rerender="formdraw,err,formdraw2,jsc" title="{!RealUserName}" >
                                    <apex:param value="{!RealUser}" assignTo="{!SelectedUser}" name="var1"/>
                                </apex:commandLink>
                            </apex:outputpanel>
                            <div class="b-tab m-active m-first">{!SelectedUserName}<span class="m-white-fix"></span></div>
                            <apex:repeat value="{!UserDt2}" var="u">
                                <apex:outputpanel layout="block" styleclass="b-tab m-has-child" rendered="{!u.HasSubordinates = true}" >
                                    <apex:commandLink action="{!SelectUser}" styleClass="m-img" status="loadStatus" oncomplete="drawchart()" rerender="formdraw,err,formdraw2,jsc">
                                        <apex:image rendered="{!ChatterEnabled = true}" value="{!u.SmallFoto}" alt="{!u.UserName}" width="45" height="45"/>
                                        <apex:param value="{!u.UserId}" assignTo="{!SelectedUser}" name="var1"/>
                                    </apex:commandLink>
                                    <apex:commandlink value="{!u.UserName}" action="{!SelectUser}" status="loadStatus" oncomplete="drawchart()" rerender="formdraw,err,formdraw2,jsc" title="{!u.UserName}" >
                                        <apex:param value="{!u.UserId}" assignTo="{!SelectedUser}" name="var1"/>
                                    </apex:commandlink>

                                </apex:outputpanel>
                                <apex:outputpanel layout="block" styleclass="b-tab" rendered="{!u.HasSubordinates = false}" >
                                    <!--  img height="45" width="45" title="{!u.UserName}" align="left" alt="{!u.UserName}" src="{!u.SmallFoto}" /> -->
                                    <apex:image rendered="{!ChatterEnabled = true}" value="{!u.SmallFoto}" alt="{!u.UserName}" width="45" height="45"/>
                                    <apex:outputText >{!u.UserName}</apex:outputText>
                                </apex:outputpanel>
                            </apex:repeat>
							<apex:outputPanel styleClass="clearAll" layout="block" rendered="{!noRecordsSearch}"><br/>
                            	<apex:outputText value="{!$Label.No_records_were_found}" />
							</apex:outputPanel>
							<apex:outputPanel id="searchTab" styleClass="clearAll" layout="block" rendered="{!isSearch}"><br/>
            					<apex:outputLabel value="{!$Label.Search} :" for="searchName" style="margin-right:10px;"/>
            					<apex:inputText value="{!searchName}" id="searchName" onkeypress="return handleKeyPress(event);"/>
            					<apex:commandButton value="{!$Label.Search}" action="{!SearchUsers}" status="loadStatus" reRender="formdraw" oncomplete="drawChart2();" style="margin-right: 10px; margin-left:10px;"/>
            				</apex:outputPanel>
                            <div class="b-pagination m-cfix">
                                <div class="m-to-left">
                                    <span class="m-bold">{!$Label.lmscons__MyTeam_Page}:</span>&nbsp;
                                    <apex:repeat value="{!tPages}" var="p">
                                        &nbsp;
                                        <apex:commandLink action="{!ChangePage}" status="loadStatus" oncomplete="drawchart()" rerender="formdraw,err,formdraw2,jsc">
                                            <apex:outputText rendered="{!p = PageNum}" ><b>{!p}</b></apex:outputText>
                                            <apex:outputText rendered="{!p != PageNum}" >{!p}</apex:outputText>
                                            <apex:param value="{!p}" assignTo="{!PageNum}" name="var2"/>
                                        </apex:commandLink>
                                    </apex:repeat>
                                </div>
                            </div>
                        </div>

                        <div class="b-tab-bodies">
                            <div class="b-tab-body">

                                <apex:outputpanel styleclass="b-section" layout="block" rendered="{!TaskList.size > 0}">
                                    <div class="b-title">{!$Label.MyTeam_Task_Manage}</div>
                                    <div class="b-manage-task">
                                        <table cellpadding="0" cellspacing="0" >
                                            <tbody>
                                                <apex:repeat value="{!TaskList}" var="a">
                                                <tr>
                                                    <td width="65">
                                                        <apex:outputText value="{0,date, MM/dd/yyyy}"><apex:param value="{!a.TaskDate}" /></apex:outputText>
                                                    </td>
                                                    <td width="120"><strong>{!a.UserName}</strong></td>
                                                    <td>{!a.TaskName}</td>
                                                    <td>{!a.Status}</td>
                                                    <td align="right" width="50"><a href="javascript:void(0)" onclick="open_task_lightbox('{!$ObjectType.Development_Plan_Task__c.Name}', '{!a.AssignmentId}')" class="m-bold">Details</a></td>
                                                </tr>
                                                </apex:repeat>
                                            </tbody>
                                        </table>
                                        <apex:outputlink rendered="{!ToManyRecords}" value="javascript:open_tasks('{!SelectedUser}');" styleClass="m-all">{!$Label.MyTeam_Task_View_All}</apex:outputlink>
                                    </div>
